<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="AccentColor">#00896d</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="AccentBrushWithOpacity" Color="{StaticResource AccentColor}" Opacity="0.7"/>
    <SolidColorBrush x:Key="AccentBrushWithLowOpacity" Color="{StaticResource AccentColor}" Opacity="0.1"/>
</ResourceDictionary>